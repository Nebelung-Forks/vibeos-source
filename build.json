{
	"minify": {
		"enabled": true,
		"fast": false
	},
	"webos": {
		"ver": "0.0.1"
	},
	"bundle": [
		{
			"path": ["basefs", "lib", "node", "buffer.js"],
			"options": {
				"expose": "buffer"
			}
		},
		{
			"path": ["basefs", "lib", "node", "mime.js"],
			"options": {
				"expose": "mime"
			}
		},
		{
			"path": ["basefs", "lib", "node", "path.js"],
			"options": {
				"expose": "path"
			}
		},
		{
			"path": ["app", "start.js"],
			"options": {
				"expose": "./start.js"
			}
		},
		{
			"path": ["basefs", "lib", "node", "require.js"],
			"options": {
				"expose": "/lib/node/require.js"
			}
		},
		{
			"path": ["basefs", "lib", "lzutf8.js"],
			"options": {
				"expose": "/lib/lzutf8.js"
			}
		},
		{
			"path": ["basefs", "lib", "node", "fs.js"],
			"options": {
				"expose": "/lib/node/fs.js"
			}
		}
	]
}